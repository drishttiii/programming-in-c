#include <stdio.h>

int main() {
    int a=0,i,n;
    printf("Enter the number to check whether its prime or not: ");
    scanf("%d",&n);
    for(i=2;i<=n-1;i++)
    {
        if(n%i==0)
        {
            a=1;
            break;
        }
    }
    if(a==0)
    {
        printf("the given number is prime\n");
    }
    else
    {
        printf("the given number is composite");
    }
    return 0;
}
